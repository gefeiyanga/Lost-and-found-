/* pages/classify/default.wxss */
page{
    background:#f5f5f5;
    box-sizing: border-box;
    padding: 0;
    margin: 0;
}
.container{
    width:100%;
    height:100%;
    background:#fff;
    position:relative;
}
.nav_left{
    width:25%;
    position:absolute;
    top:0rpx;
    left:0rpx;
    background: #f5f5f5;
    text-align: center;
}
.nav_left_items{
    height:120rpx;
    border-bottom:1rpx solid #dedede;
    line-height:120rpx;
}

.container .nav_left .nav_left_items.actived{
    background:#fff;
    color: #3385ff;
    border-left:2px solid #3385ff ;
}
.nav_right{
    width:75%;
    height:1334rpx;
    position:absolute; 
    top:0rpx;
    left:187.5rpx;
    background: #fff;
    text-align: center;
}
.nav_right .nav_right_items{
    float:left;
    width: 33.3%;
    height:120rpx;
    text-align: center;
    line-height: 120rpx;
}
image{
  vertical-align: middle;
  width: 80rpx;
  height:80rpx;
}